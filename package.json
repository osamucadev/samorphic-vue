{
  "name": "samorphic-vue",
  "version": "1.2.0",
  "dependencies": {
    "sass": "^1.89.1"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitejs/plugin-vue-jsx": "^4.2.0",
    "storybook": "latest",
    "typescript": "^5.8.3",
    "vite": "^4.5.14",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-lib-inject-css": "^2.2.2",
    "vitepress": "^1.6.3",
    "vue-tsc": "^1.8.27"
  },
  "type": "module",
  "scripts": {
    "build": "vite build",
    "dev:watch": "vite build --watch",
    "docs:dev": "vitepress dev docs",
